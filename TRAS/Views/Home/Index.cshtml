@{
    ViewBag.Title = "Home Page";
}
@section searchScript{
    <script src="~/Scripts/jquery.bootgap.min.js"></script>
    <script src="~/Scripts/app/Search/search.js"></script>
    <script src="~/Scripts/app/Search/searchModules.js"></script>
  }
<div itemscope itemtype="http://schema.org/Thing">
    <div class="jumbotron">
        <form class="navbar-form pull-right" role="search" name="searchForm" id="searchForm">
            <div class="form-group">
                <input type="text" class="form-control" id="search" placeholder="Search">
            </div>
            <button type="submit" class="btn btn-default" id="submitBtn"><i class="glyphicon glyphicon-search"></i></button>
        </form>
        <h1 itemprop="name" style="font-family:CFParis;">TRAS</h1>

        <p itemprop="description" class="lead"><small><em>A school project developed in the Web Applications Development class.</em></small></p>
    </div>

    <div class="body-content">
        <div class="container">
            <div class="row" id="main">
                <img itemprop="photo" src="Content/images/mainimg.jpg" class="img-responsive max-width: 100%;" id="mainImg">
                <div class="col-md-3" id="content">
                    <ul id="items" class="nav nav-list bs-docs-sidenav thumbnail" style="padding:10px;"></ul>
                </div>
                <div class="col-md-9" style="background-color: #eeeeee; border-radius: 6px; margin:0 auto;" id="entry-list">
                    <ul id='entries'></ul>
                    <div id="paginator"></div>
                </div>
            </div>
        </div>
    </div>
</div>